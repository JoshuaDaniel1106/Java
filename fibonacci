package day4;

import java.util.Scanner;

public class fibonacci {
	public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
    int a = scan.nextInt();   
    int x = 0, y = 1;   
    System.out.println("Fibonacci Series up to " + a + " terms:");  
    for (int i = 1; i <= a; i++) {
        System.out.println(x + " ");
        int z = x + y;
        x = y;
        y = z;
    }
}
}
